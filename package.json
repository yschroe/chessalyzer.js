	"devDependencies": {
		"dts-bundle-webpack": "^1.0.2",
		"eslint": "^7.31.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-plugin-import": "^2.23.4",
		"eslint-webpack-plugin": "^2.5.4",
		"ink-docstrap": "^1.3.2",
		"jsdoc": "^3.6.7",
